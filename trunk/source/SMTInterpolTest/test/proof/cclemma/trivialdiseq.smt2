(set-option :print-success false)
(set-option :produce-proofs true)
(set-option :proof-check-mode true)
(set-info :status unsat)
(set-logic QF_UFLIA)
(declare-fun f (Int) Int)
(declare-fun x () Int)
(declare-fun y () Int)
(assert (and (= x (f x)) (= x y) (= (f y) (+ x 1))))
(check-sat)
(get-proof)
(exit)
