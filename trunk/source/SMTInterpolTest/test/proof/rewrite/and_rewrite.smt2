(set-option :produce-proofs true)
(set-option :proof-check-mode true)
(set-option :model-check-mode true)
(set-option :print-terms-cse false)

(set-logic QF_UF)
(declare-fun p () Bool)
(declare-fun q () Bool)
(declare-fun r () Bool)

(push 1)
(assert (and p q r))
(assert (or (not r) (not q) (not p)))
(check-sat)
(get-proof)
(pop 1)

(push 1)
(assert (and p (not q) (not r)))
(assert (or r q (not p)))
(check-sat)
(get-proof)
(pop 1)
