(set-option :produce-proofs true)
(set-option :proof-check-mode true)
(set-option :model-check-mode true)
(set-option :print-terms-cse false)

(set-logic QF_AX)
(declare-sort U 0)
(declare-fun a () (Array U U))
(declare-fun i () U)
(declare-fun v () U)

(push 1)
(assert (= (store a i v) a))
(assert (not (= (select a i) v)))
(check-sat)
(get-proof)
(pop 1)

(push 1)
(assert (not (= (store a i v) a)))
(assert (= (select a i) v))
(check-sat)
(get-proof)
(pop 1)
