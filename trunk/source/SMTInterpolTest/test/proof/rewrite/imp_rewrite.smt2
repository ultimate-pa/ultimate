(set-option :produce-proofs true)
(set-option :proof-check-mode true)
(set-option :model-check-mode true)
(set-option :print-terms-cse false)

(set-logic QF_UF)
(declare-fun p () Bool)
(declare-fun q () Bool)
(declare-fun r () Bool)

(push 1)
(assert (=> p q r))
(assert p)
(assert q)
(assert (not r))
(check-sat)
(get-proof)
(pop 1)

(push 1)
(assert (=> (not p) q (not r)))
(assert (not p))
(assert q)
(assert r)
(check-sat)
(get-proof)
(pop 1)
