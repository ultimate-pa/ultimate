(set-option :produce-proofs true)
(set-option :proof-check-mode true)
(set-option :model-check-mode true)
(set-option :print-terms-cse false)

(set-logic QF_LIRA)
(declare-const i Int)
(declare-const x Real)

(push 1)
(assert (not (= (* 3 i) (* i 3))))
(check-sat)
(get-proof)
(pop 1)

(push 1)
(assert (not (= (* 3 5 i) (* i 5 3))))
(check-sat)
(get-proof)
(pop 1)

(push 1)
(assert (not (= (* 3 i 5) (* i 15 1))))
(check-sat)
(get-proof)
(pop 1)

(push 1)
(assert (not (= (* 3 x) (* x 3))))
(check-sat)
(get-proof)
(pop 1)

(push 1)
(assert (not (= (* 3 5 x) (* x 5 3))))
(check-sat)
(get-proof)
(pop 1)

(push 1)
(assert (not (= (* 3 x 5) (* x 15 1))))
(check-sat)
(get-proof)
(pop 1)
