(set-option :produce-proofs true)
(set-option :proof-check-mode true)
(set-logic QF_UFDT)
(declare-sort U 0)
(declare-datatypes ( (List 0) (Nat 0) ) (
 ( (nil) (cons (car Nat) (cdr List) ))
 ( (zero) (succ (pred Nat)) )
))
(declare-const x0 Nat)
(declare-const x1 Nat)
(declare-const x2 Nat)
(declare-const y Nat)

(assert ((_ is succ) x0))
(assert (= (pred x1) y))
(assert (not (= x2 (succ y))))
(assert (= x0 x1))
(assert (= x1 x2))
(check-sat)
(get-proof)
