(set-option :produce-proofs true)
(set-option :proof-check-mode true)

(set-logic QF_ALIA)
(declare-sort U 0)
(declare-fun v1 () U)
(declare-fun v2 () U)
(declare-fun i2 () Int)
(declare-fun a () (Array Int U))
(declare-fun b () (Array Int U))
(declare-fun c () (Array Int U))
(declare-fun d () (Array Int U))
(define-fun constU ((v U)) (Array Int U) ((as const (Array Int U)) v))

(assert (= a (store (constU v1) i2 v2)))
(assert (= b (store (constU v2) i2 v2)))
(assert (not (= b a)))
(assert (not (= c (constU v2))))
(assert (not (= c d)))
(check-sat)
(get-model)
(exit)

