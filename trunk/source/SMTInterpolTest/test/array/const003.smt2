(set-option :produce-proofs true)
(set-option :proof-check-mode true)
(set-option :print-terms-cse false)

(set-logic QF_ALIA)
(declare-sort U 0)
(declare-fun v1 () U)
(declare-fun v2 () U)
(declare-fun i1 () Int)
(declare-fun i2 () Int)
(declare-fun w1 () U)
(declare-fun w2 () U)
(declare-fun a () (Array Int U))

(assert (= (store a i1 w1) ((as const (Array Int U)) v1)))
(assert (= (store a i2 w2) ((as const (Array Int U)) v2)))
(assert (or (not (= v1 v2)) (not (= v1 w1)) (not (= v2 w2))))
(check-sat)
(get-proof)
(exit)