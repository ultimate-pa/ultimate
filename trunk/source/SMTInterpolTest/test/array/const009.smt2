(set-option :produce-proofs true)
(set-option :proof-check-mode true)
(set-option :print-terms-cse false)

(set-logic QF_ALIA)
(declare-sort U 0)
(declare-fun v () U)
(declare-fun w1 () U)
(declare-fun w2 () U)
(declare-fun i () Int)
(declare-fun k () Int)
(declare-fun a () (Array Int U))

(assert (= a (store ((as const (Array Int U)) v) i w1)))
(assert (not (= i k)))
(assert (= ((as const (Array Int U)) v) (store a k w2)))
(assert (not (= v w1)))
(check-sat)
(get-proof)
(exit)
