(set-option :produce-proofs true)
(set-logic QF_UFNIA)

(declare-const x Int)
(declare-const d Int)
(declare-const a Int)
(declare-const b Int)

(push 1)
(assert (not (= x (+ (* d (div x d)) (mod x d)))))
(assert (not (= d 0)))
(check-sat)
(get-proof)
(pop 1)

