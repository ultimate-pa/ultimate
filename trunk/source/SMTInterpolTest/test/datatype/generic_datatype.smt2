(set-info :smt-lib-version 2.6)
(set-logic QF_DT)
(set-info :category "crafted")
(set-info :status sat)
(declare-datatypes ((nat 0) (list 1) (tree 2))
  (((succ (pred nat)) (zero))
   (par (X) ((cons (car X) (cdr (list X))) (null)))
   (par (X Y) ((node (key X) (children (list (tree X Y)))) (leaf (data Y))))
  ))

(declare-fun x1 () nat)
(declare-fun x2 () (list nat))
(declare-fun x3 () (tree nat nat))
(assert (= x3 (node (succ zero) (cons ((as leaf (tree nat nat)) zero) (cons ((as leaf (tree nat nat)) (succ zero)) (as null (list (tree nat nat))))))))
(check-sat)
(exit)
