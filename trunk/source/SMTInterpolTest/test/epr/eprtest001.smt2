(set-option :print-success false)
(set-option :produce-proofs false)
(set-option :interpolant-check-mode true)

(set-logic UF)
(set-info :source |Simple EPR example.|)
(set-info :smt-lib-version 2.0)
(set-info :category "crafted")
(set-info :status unsat)
(declare-sort V 0)
(declare-fun E (V V V) Bool)
(declare-fun a () V)
(declare-fun b () V)
(declare-fun c () V)
(declare-fun d () V)
(assert (forall ((x V) (y V) (z V) (u V)) (or (E x y y) (E y u a))))
(assert (not (E a b c)))
(assert (not (E a c c)))
(assert (not (E c d a)))
(assert (not (E c d d)))
(check-sat)
(exit)
