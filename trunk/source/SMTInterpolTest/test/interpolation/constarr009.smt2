(set-option :produce-proofs true)
(set-option :proof-check-mode true)
(set-option :interpolant-check-mode true)
(set-option :print-terms-cse false)

(set-logic QF_AUFLIA)
(declare-fun u1 () Int)
(declare-fun u2 () Int)
(declare-fun u3 () Int)
(declare-fun u4 () Int)
(declare-fun u5 () Int)
(declare-fun u6 () Int)
(declare-fun v () Int)
(declare-fun w () Int)
(declare-fun k1 () Int)
(declare-fun k2 () Int)
(declare-fun k3 () Int)
(declare-fun k4 () Int)
(declare-fun k5 () Int)
(declare-fun k6 () Int)
(declare-fun s1 () (Array Int Int))
(declare-fun s2 () (Array Int Int))
(declare-fun s3 () (Array Int Int))
(declare-fun p (Int) Bool)

(assert (! (and (p v) (= ((as const (Array Int Int)) v) (store s1 k1 u1)) (= s3 (store (store s2 k3 u3) k4 u4))) :named A))
(assert (! (and (not (p w)) (= s2 (store s1 k2 u2)) (= ((as const (Array Int Int)) w) (store (store s3 k5 u5) k6 u6))) :named B))

(check-sat)
(get-proof)
(get-interpolants A B)
(exit)
