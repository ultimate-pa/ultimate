(set-option :produce-interpolants true)
(set-option :interpolant-check-mode true)
(set-logic QF_AUFLIA)
(declare-sort U 0)
(declare-const ar1 (Array Int Int))
(declare-const ar2 (Array Int Int))
(declare-const ar (Array Int Int))
(declare-const s Int)
(declare-const t U)
(declare-const t2 U)
(declare-const a Int)
(declare-const b Int)
