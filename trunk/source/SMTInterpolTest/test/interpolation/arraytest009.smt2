(set-option :produce-proofs true)
(set-option :proof-check-mode true)
(set-option :interpolant-check-mode true)

(set-logic QF_AX)
(declare-sort U 0)
(declare-fun i () U)
(declare-fun j () U)
(declare-fun k1 () U)
(declare-fun k2 () U)
(declare-fun v1 () U)
(declare-fun v2 () U)
(declare-fun a () (Array U U))
(declare-fun b () (Array U U))
(declare-fun s1 () (Array U U))
(declare-fun s2 () (Array U U))

(assert (! (and (not (= (select a i) (select b j))) (not (= k1 j)) (= i j)
                (= a s1) (= s2 b))
	:named B))
(assert (! (and (= s1 (store (store s2 k1 v1) k2 v2))
	   (not (= k2 i)))
	:named A))

(check-sat)
(set-option :print-terms-cse false)
(get-proof)
(get-interpolants A B)
(exit)
