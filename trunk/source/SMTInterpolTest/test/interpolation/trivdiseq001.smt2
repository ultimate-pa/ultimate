(set-option :produce-interpolants true)
(set-option :interpolant-check-mode true)
(set-logic QF_UFLIA)
(declare-sort U 0)
(declare-const s Int)
(declare-const t U)
(declare-const t2 U)
(declare-const a Int)
(declare-const b Int)
(declare-fun f (U) Int)

(assert (! (= (+ s (* 2 a)) (f t)) :named A))
(assert (! (= (f t) (+ s (* 2 b) 1)) :named B))
(check-sat)
(get-proof)
(get-interpolants A B)
