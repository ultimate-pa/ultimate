(set-option :produce-proofs true)
(set-option :proof-check-mode true)
(set-option :interpolant-check-mode true)
(set-option :print-terms-cse false)

(set-logic QF_AUFLIA)
(declare-fun v () Int)
(declare-fun w1 () Int)
(declare-fun w2 () Int)
(declare-fun w3 () Int)
(declare-fun i () Int)
(declare-fun k1 () Int)
(declare-fun k2 () Int)
(declare-fun k3 () Int)
(declare-fun p (Int) Bool)
(declare-fun p2 (Int) Bool)
(declare-fun p3 (Int) Bool)
(declare-fun a () (Array Int Int))
(declare-fun s () (Array Int Int))

(assert (! (and (p (select a i)) (= s (store a k1 w1))
(not (= i k1)) (p2 i) (p3 i)) :named A))
(assert (! (and (not (p v)) (= s (store (store ((as const (Array Int Int)) v) k2 w2) k3 w3))
(not (p2 k2)) (not (p3 k3))) :named B))

(check-sat)
(get-proof)
(get-interpolants A B)
(exit)
