(set-option :produce-interpolants true)
(set-option :interpolant-check-mode true)
(set-logic QF_UFLIA)
(declare-fun f (Int) Int)
(declare-fun b () Int)
(assert (! (= (f b) 0) :named root))
(assert (! (= 0 b) :named left))
(assert (! (= b 1) :named right))
(check-sat)
(get-interpolants left (right) root)
