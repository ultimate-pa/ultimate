(set-logic QF_AX)
(declare-sort Index 0)
(declare-sort Element 0)
(declare-fun S () (Array Index Element))
(declare-fun SS () (Array Index Element))
(declare-fun a () Index)
(declare-fun b () Index)
(declare-fun v () Element)
(assert (and (= S (store SS a v)) (not (= (select S b) (select (store S a v) b)))))
(check-sat)
(exit)
