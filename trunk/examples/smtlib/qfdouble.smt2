(set-option :produce-proofs true)
(set-logic AUFLIA)
(declare-fun P ( Int) Bool)
(declare-fun Q ( Int) Bool)
(assert (! (and (forall ((x Int)) (P x)) (forall ((x Int)) (not (Q x)))) :named IP_0))
(assert (! (and (forall ((x Int)) (=> (P x) (Q x))) (P 3)) :named IP_1))
(check-sat)
(get-interpolants IP_0 IP_1)
(exit)
