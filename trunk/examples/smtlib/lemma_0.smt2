(set-option :produce-proofs true)
(set-info :status unsat)
(set-logic QF_LIA)
(declare-fun z () Int)
(declare-fun x () Int)
(declare-fun y () Int)
(assert (! (<= (+ x (* (- 0 2) z)) 1) :named IP_0))
(assert (! (<= (+ x (* (- 0 2) y)) 0) :named IP_1))
(assert (! (>= (+ (* 2 x) (+ (* (- 0 2) y) (* (- 0 2) z))) 2) :named IP_2))
(assert (! true :named IP_3))
(check-sat)
(get-interpolants IP_0 IP_1 IP_2 IP_3)
(exit)
