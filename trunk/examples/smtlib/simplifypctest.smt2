(set-logic AUFLIA)
(declare-fun a () Int)
(declare-fun b () Int)
(declare-fun c () Int)
(declare-fun f (Int) Int)
(declare-fun g (Int Int) Int)
(assert (forall ((?x Int)) (! (= (f (g ?x a)) a) :pattern ((f (g ?x a))))))
(assert (not (= (f b) a)))
(assert (or (= b (g b a)) (and (= b (g c c)) (= c a))))
(check-sat)
(exit)
