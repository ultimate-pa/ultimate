(set-option :produce-proofs true)
(set-info :source "{ Example with quantifiers which should allow for quantifier-free interpolation }")
(set-logic AUFLIA)
(declare-fun P ( Int) Int)
(declare-fun a () Int)
(declare-fun y () Int)
(assert (! (and (forall ((x Int)) (= (P x) (+ x 1))) (= a (+ y 1))) :named IP_0))
(assert (! (not (= (P y) a)) :named IP_1))
(check-sat)
(get-interpolants IP_0 IP_1)
(exit)
