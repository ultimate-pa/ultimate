(set-option :produce-proofs true)
(set-logic AUFLIRA)
(declare-fun Mp () (Array Int Real))
(declare-fun M () (Array Int Real))
(declare-fun x () Real)
(declare-fun a () Int)
(declare-fun b () Int)
(declare-fun c () Int)
(assert (! (= Mp (store M a x)) :named IP_0))
(assert (! (and (distinct b c) (distinct (select Mp b) (select M b)) (distinct (select Mp c) (select M c))) :named IP_1))
(check-sat)
(get-interpolants IP_0 IP_1)
(exit)
