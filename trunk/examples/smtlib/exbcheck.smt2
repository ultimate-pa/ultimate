(set-option :produce-proofs true)
(set-logic AUFLIRA)
(declare-fun M () (Array Int Real))
(declare-fun Mp () (Array Int Real))
(declare-fun a () Int)
(declare-fun b () Int)
(declare-fun c () Int)
(declare-fun x () Real)
(assert (and (distinct b c) (distinct (select Mp b) (select M b)) (distinct (select Mp c) (select M c)) (forall ((b Int)) (or (= (select Mp b) (select M b)) (forall ((c Int)) (or (= b c) (= (select Mp c) (select M c))))))))
(check-sat)
(exit)
