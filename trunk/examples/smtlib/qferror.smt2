(set-option :produce-proofs true)
(set-info :source "{ Test for Z3 Proof bug }")
(set-logic AUFLIA)
(declare-fun P ( Int) Bool)
(declare-fun Q ( Int) Bool)
(assert (! (forall ((x Int)) (and (exists ((x Int)) (P x)) (Q x))) :named IP_0))
(assert (! (P 3) :named IP_1))
(check-sat)
(get-interpolants IP_0 IP_1)
(exit)
