(set-logic QF_AUFLIA)
(set-info :status unsat)
(declare-fun i () Int)
(declare-fun j () Int)
(declare-fun v () Int)
(declare-fun a () (Array Int Int))
(declare-fun b () (Array Int Int))
(declare-fun c () (Array Int Int))
(assert (= b (store a i v)))
(assert (= c (store a i v)))
(assert (= v (select b j)))
(assert (not (= i j)))
(assert (not (= v (select a j))))
(check-sat)
(exit)
