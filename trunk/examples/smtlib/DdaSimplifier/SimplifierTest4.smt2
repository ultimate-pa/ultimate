(set-logic QF_UFLIA)
(set-info :author pomrehn@informatik.uni-freiburg.de)
(set-info :category "crafted")
(set-info :status unsat)
(set-info :simplyfied (or A B))
(set-info :source "{ One disjunct is non-relaxing and can be simplified to false.}")
(set-option :strong-context-simplifier true)
(declare-fun A () Bool)
(declare-fun B () Bool)
(simplify (or A A B))
(exit)