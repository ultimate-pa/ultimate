(set-option :produce-proofs true)
(set-info :source "{ Simple test for interpolation with quantified formulae. Interpolant is hard to find:
(forall (?x Int) (not (= (g ?x) s2))) }")
(set-logic AUFLIA)
(declare-fun f ( Int) Int)
(declare-fun g ( Int) Int)
(declare-fun s1 () Int)
(declare-fun s2 () Int)
(declare-fun b () Int)
(assert (! (and (forall ((x Int)) (= (f (g x)) s1)) (not (= (f s2) s1))) :named IP_0))
(assert (! (= (g b) s2) :named IP_1))
(check-sat)
(get-interpolants IP_0 IP_1)
(exit)
