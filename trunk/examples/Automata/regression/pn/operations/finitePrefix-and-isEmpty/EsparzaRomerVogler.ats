		({s2} t3 {s6 s7})
		({s4} t4 {s6 s7})
		({s3} t5 {s8 s9})
		({s5} t6 {s8 s9})
		({s6 s8} t7 {s10})
		({s7 s9} t8 {s11})
		({s10 s11} t9 {s12})
	},
	initialMarking = {s1},
	acceptingPlaces = {s12}
);
