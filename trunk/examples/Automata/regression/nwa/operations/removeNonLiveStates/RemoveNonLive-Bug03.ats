NestedWordAutomaton result = removeNonLiveStates(nwa);
print(numberOfStates(result));
assert(numberOfStates(result) == 2);

NestedWordAutomaton nwa = (
	callAlphabet = {"a0" "a1" },
	internalAlphabet = {"a0" "a1" },
	returnAlphabet = {"a0" "a1" },
	states = {"q1" "q0" },
	initialStates = {"q0" },
	finalStates = {"q0" },
	callTransitions = {
		 ("q1" "a0" "q0")
	},
	internalTransitions = {
		 ("q0" "a1" "q1")
	},
	returnTransitions = {
		 ("q0" "q1" "a1" "q0")
	}
);