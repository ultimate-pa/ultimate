RabinPetriNet testBasic = (
	alphabet = {a b c},
	places = {p1 p2},
	transitions = { ({p1} a {p1}) ({p1} b {p2})	({p2} c {p2}) },
	initialMarking = {p1},
	acceptingPlaces = {p1 p2},
	finitePlaces = {p1}
);

assert(!rabinIsEmpty(testBasic));


