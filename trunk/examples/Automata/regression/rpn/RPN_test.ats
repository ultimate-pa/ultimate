// Parser Test

RabinPetriNet testNet = (
	alphabet = {a b},
	places = {p1 p2},
	transitions = { ({p1} a {p1}) ({p2} b {p2})	},
	initialMarking = {p1 p2},
	acceptingPlaces = {p1},
	finitePlaces = {p2}
);

assert(!rabinIsEmpty(testNet));

assert(rabinAccepts(testNet, [a a b , a]));
assert(!rabinAccepts(testNet, [, b]));
assert(!rabinAccepts(testNet, [a a  , b a]));
