<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<benchmark tool="shell" memlimit="8GB" timelimit="300" hardtimelimit="320">
  <resultfiles></resultfiles>

  <option name="success">1 tests successful</option>
  <option name="failure">1 tests failed</option>

  <rundefinition name="PetriOwickiGriesTestSuite-WithAssert">
    <option name="command">${benchmark_path}/../run-test.sh Library-TraceCheckerUtilsTest de.uni_freiburg.informatik.ultimate.lib.tracecheckerutils.owickigries.PetriOwickiGriesTestSuite ${inputfile_name} true</option>
  </rundefinition>

  <rundefinition name="NaiveOwickiGriesTestSuite-WithAssert">
    <option name="command">${benchmark_path}/../run-test.sh Library-TraceCheckerUtilsTest de.uni_freiburg.informatik.ultimate.lib.tracecheckerutils.owickigries.NaiveOwickiGriesTestSuite ${inputfile_name} true</option>
  </rundefinition>

  <rundefinition name="PetriOwickiGriesTestSuite-NoAssert">
    <option name="command">${benchmark_path}/../run-test.sh Library-TraceCheckerUtilsTest de.uni_freiburg.informatik.ultimate.lib.tracecheckerutils.owickigries.PetriOwickiGriesTestSuite ${inputfile_name} false</option>
  </rundefinition>

  <rundefinition name="NaiveOwickiGriesTestSuite-NoAssert">
    <option name="command">${benchmark_path}/../run-test.sh Library-TraceCheckerUtilsTest de.uni_freiburg.informatik.ultimate.lib.tracecheckerutils.owickigries.NaiveOwickiGriesTestSuite ${inputfile_name} false</option>
  </rundefinition>

  <tasks name="regression-MemSafety">
    <include>../../../trunk/examples/concurrent/OwickiGries/PetriPrograms/MemSafety.*.ats</include>
  </tasks>

  <tasks name="regression-ConcurrencySafety">
    <include>../../../trunk/examples/concurrent/OwickiGries/PetriPrograms/ConcurrencySafety.*.ats</include>
  </tasks>

</benchmark>
