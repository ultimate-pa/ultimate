<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<benchmark tool="ultimategemcutter" memlimit="8GB" timelimit="900" hardtimelimit="920" cpuCores="2">
  <resultfiles></resultfiles>

  <option name="&#45;-force-no-wrapper" />

  <option name="-s">${benchmark_path}/../../../trunk/examples/settings/gemcutter/NewStatesSleep.epf</option>
  <option name="&#45;-traceabstraction.dfs.order.used.in.por">LOOP_LOCKSTEP</option>

  <rundefinition name="NONE">
    <option name="&#45;-traceabstraction.additional.conditional.commutativity.checking">NONE</option>
  </rundefinition>

  
   <rundefinition name="CE-Default-Generator">
    <option name="&#45;-traceabstraction.additional.conditional.commutativity.checking">COUNTEREXAMPLE</option>
    <option name="&#45;-traceabstraction.criterion.for.conditional.commutativity.checking">DEFAULT</option>
	<option name="&#45;-traceabstraction.tracecheck.mode.for.conditional.commutativity">GENERATOR</option>
  </rundefinition>
  
    <rundefinition name="CE-Default-GeneratorWithContext">
    <option name="&#45;-traceabstraction.additional.conditional.commutativity.checking">COUNTEREXAMPLE</option>
    <option name="&#45;-traceabstraction.criterion.for.conditional.commutativity.checking">DEFAULT</option>
	<option name="&#45;-traceabstraction.tracecheck.mode.for.conditional.commutativity">GENERATOR_WITH_CONTEXT</option>
  </rundefinition>
  
    <rundefinition name="CE-Default-SymbolicRelation">
    <option name="&#45;-traceabstraction.additional.conditional.commutativity.checking">COUNTEREXAMPLE</option>
    <option name="&#45;-traceabstraction.criterion.for.conditional.commutativity.checking">DEFAULT</option>
	<option name="&#45;-traceabstraction.tracecheck.mode.for.conditional.commutativity">SYMBOLIC_RELATION</option>
  </rundefinition>
  
  <rundefinition name="CE-SleepSet">
    <option name="&#45;-traceabstraction.additional.conditional.commutativity.checking">COUNTEREXAMPLE</option>
    <option name="&#45;-traceabstraction.criterion.for.conditional.commutativity.checking">SLEEP_SET</option>
  </rundefinition>
  
  <rundefinition name="CE-Loop">
    <option name="&#45;-traceabstraction.additional.conditional.commutativity.checking">COUNTEREXAMPLE</option>
    <option name="&#45;-traceabstraction.criterion.for.conditional.commutativity.checking">LOOP</option>
  </rundefinition>
  
   <rundefinition name="CE-Forward">
    <option name="&#45;-traceabstraction.additional.conditional.commutativity.checking">COUNTEREXAMPLE</option>
    <option name="&#45;-traceabstraction.criterion.for.conditional.commutativity.checking">FORWARD</option>
  </rundefinition>
  
  <rundefinition name="CE-Random-20-213">
    <option name="&#45;-traceabstraction.additional.conditional.commutativity.checking">COUNTEREXAMPLE</option>
    <option name="&#45;-traceabstraction.criterion.for.conditional.commutativity.checking">RANDOM</option>
    <option name="&#45;-traceabstraction.probability.for.random.criterion">20</option>
    <option name="&#45;-traceabstraction.seed.for.random.criterion">213</option>
  </rundefinition>
  
   <rundefinition name="CE-Random-20-546">
    <option name="&#45;-traceabstraction.additional.conditional.commutativity.checking">COUNTEREXAMPLE</option>
    <option name="&#45;-traceabstraction.criterion.for.conditional.commutativity.checking">RANDOM</option>
    <option name="&#45;-traceabstraction.probability.for.random.criterion">20</option>
    <option name="&#45;-traceabstraction.seed.for.random.criterion">546</option>
  </rundefinition>
  
  <rundefinition name="CE-Random-50-213">
    <option name="&#45;-traceabstraction.additional.conditional.commutativity.checking">COUNTEREXAMPLE</option>
    <option name="&#45;-traceabstraction.criterion.for.conditional.commutativity.checking">RANDOM</option>
    <option name="&#45;-traceabstraction.probability.for.random.criterion">50</option>
    <option name="&#45;-traceabstraction.seed.for.random.criterion">213</option>
  </rundefinition>


  <rundefinition name="IA-Default">
    <option name="&#45;-traceabstraction.additional.conditional.commutativity.checking">IA</option>
    <option name="&#45;-traceabstraction.criterion.for.conditional.commutativity.checking">DEFAULT</option>
  </rundefinition>
  
  <rundefinition name="IA-SleepSet">
    <option name="&#45;-traceabstraction.additional.conditional.commutativity.checking">IA</option>
    <option name="&#45;-traceabstraction.criterion.for.conditional.commutativity.checking">SLEEP_SET</option>
  </rundefinition>
  
   <rundefinition name="IA-Loop">
    <option name="&#45;-traceabstraction.additional.conditional.commutativity.checking">IA</option>
    <option name="&#45;-traceabstraction.criterion.for.conditional.commutativity.checking">LOOP</option>
  </rundefinition>
  
   <rundefinition name="DFS-Default-Limit5">
    <option name="&#45;-traceabstraction.additional.conditional.commutativity.checking">DFS</option>
    <option name="&#45;-traceabstraction.criterion.for.conditional.commutativity.checking">DEFAULT</option>
    <option name="&#45;-traceabstraction.use.limited.checks.recommended.for.dfs">true</option>
    <option name="&#45;-traceabstraction.limit.for.limited.checks">5</option>
  </rundefinition>

  <rundefinition name="DFS-SleepSet-Limit5">
    <option name="&#45;-traceabstraction.additional.conditional.commutativity.checking">DFS</option>
    <option name="&#45;-traceabstraction.criterion.for.conditional.commutativity.checking">SLEEP_SET</option>
    <option name="&#45;-traceabstraction.use.limited.checks.recommended.for.dfs">true</option>
    <option name="&#45;-traceabstraction.limit.for.limited.checks">5</option>
  </rundefinition>
  
   <rundefinition name="DFS-Loop-Limit5">
    <option name="&#45;-traceabstraction.additional.conditional.commutativity.checking">DFS</option>
    <option name="&#45;-traceabstraction.criterion.for.conditional.commutativity.checking">LOOP</option>
    <option name="&#45;-traceabstraction.use.limited.checks.recommended.for.dfs">true</option>
    <option name="&#45;-traceabstraction.limit.for.limited.checks">5</option>
  </rundefinition>


  <tasks name="single">
    <option name="-tc">${benchmark_path}/../../../trunk/examples/toolchains/AutomizerCInline.xml</option>
    <include>${benchmark_path}/../../../trunk/examples/svcomp/weaver/parallel-misc-2-unrolled-atomic.wvr.yml</include>
    <propertyfile expectedverdict="true">${benchmark_path}/../../../trunk/examples/svcomp/properties/unreach-call.prp</propertyfile>
  </tasks>

  <tasks name="SVCOMP-ConcurrencySafety-Main">
    <option name="-tc">${benchmark_path}/../../../trunk/examples/toolchains/AutomizerCInline.xml</option>
    <includesfile>${benchmark_path}/../../../trunk/examples/svcomp/ConcurrencySafety-Main.set</includesfile>
    <propertyfile>${benchmark_path}/../../../trunk/examples/svcomp/properties/unreach-call.prp</propertyfile>
  </tasks>

  <tasks name="Weaver-Boogie">
    <option name="-tc">${benchmark_path}/../../../trunk/examples/toolchains/AutomizerBplInline.xml</option>
    <includesfile>${benchmark_path}/../../../trunk/examples/concurrent/bpl/weaver-benchmarks/WeaverBenchmarks.set</includesfile>
    <propertyfile>${benchmark_path}/../../../trunk/examples/svcomp/properties/unreach-call.prp</propertyfile>
  </tasks>
</benchmark>
