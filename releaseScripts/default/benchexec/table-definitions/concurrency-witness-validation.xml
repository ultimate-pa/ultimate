<?xml version="1.0" ?>
<!DOCTYPE table PUBLIC "+//IDN sosy-lab.org//DTD BenchExec table 1.10//EN" "https://www.sosy-lab.org/benchexec/table-1.10.dtd">
<table>
  <!-- baseline -->
  <result filename="concurrency-witness-validation.*.results.verify.xml">
    <column title="status"/>
    <column title="cputime" displayTitle="CPU Time"/>
    <column title="memory"  displayTitle="Memory" sourceUnit="B" displayUnit="MB"/>

    <column title="iterations" displayTitle="Refinement Iterations">OverallIterations:\s*(\d+),</column>
  </result>

  <!-- validation of witnesses generated by goblint -->
  <result filename="concurrency-witness-validation.*.results.validate-goblint-witnesses.xml">
    <column title="status"/>
    <column title="cputime" displayTitle="CPU Time"/>
    <column title="memory"  displayTitle="Memory" sourceUnit="B" displayUnit="MB"/>

    <column title="iterations" displayTitle="Refinement Iterations">OverallIterations:\s*(\d+),</column>
    <column title="errorType" displayTitle="Violation">((?&lt;=a call to )reach_error(?= is reachable)|invariant(?= of correctness witness can be violated))</column>
  </result>
</table>
